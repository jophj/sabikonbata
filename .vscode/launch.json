  "version": "0.2.0",
  "configurations": [
    {
      "name": "(OSX) Launch",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/target/debug/sabikonbata",
      "args": ["."],
      "env": {
        "RUST_BACKTRACE": "full"
      },
      "cwd": "${workspaceRoot}",
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'sabikonbata'",
      "cargo": {
        "args": [
          "build",
          "--bin=sabikonbata",
          "--package=sabikonbata"
        ],
        "filter": {
          "name": "sabikonbata",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'sabikonbata'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=sabikonbata",
          "--package=sabikonbata"
        ],
        "filter": {
          "name": "sabikonbata",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}